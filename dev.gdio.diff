diff --git a/.gitconfig b/.gitconfig
new file mode 100644
--- /dev/null
+++ ./.gitconfig
@@ -0,0 +1,11 @@
+[remote "origin"]
+	url = git@github.com:huangyingw/vim-snipmate.git
+	fetch = +refs/heads/*:refs/remotes/origin/*
+[remote "garbas"]
+	url = git@github.com:garbas/vim-snipmate.git
+	fetch = +refs/heads/*:refs/remotes/garbas/*
+[push]
+    remote = origin
+[gsync]
+    remote = garbas
+    target = garbas/master
diff --git a/plugin/snipMate.vim b/plugin/snipMate.vim
--- ./plugin/snipMate.vim
+++ ./plugin/snipMate.vim
@@ -57,10 +57,6 @@ endif
 let g:snipMate['no_match_completion_feedkeys_chars'] =
 			\ get(g:snipMate, 'no_match_completion_feedkeys_chars', "\t")
 
-if !exists('g:snipMate.snippet_version')
-	echom 'The legacy SnipMate parser is deprecated. Please see :h SnipMate-deprecate.'
-endif
-
 " Add default scope aliases, without overriding user settings
 let g:snipMate.scope_aliases = get(g:snipMate, 'scope_aliases', {})
 if exists('g:snipMate_no_default_aliases')
